{
  "name": "apartment-helper",
  "version": "0.1.0",
  "description": "Helps me find an apartment.",
  "homepage": "https://github.com/efortner/apartment-helper#readme",
  "bugs": {
    "url": "https://github.com/efortner/apartment-helper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/efortner/apartment-helper.git"
  },
  "license": "MIT",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "prepare": "husky",
    "clean": "rm -rf dist node_modules coverage src/**/*.guard.ts data/*.json mcp.json",
    "lint": "eslint . && prettier --check .",
    "format": "eslint . --fix && prettier --write .",
    "build:guards": "ts-auto-guard src/types/*.ts",
    "build:install": "node bin/install-mcp.mjs",
    "build": "npm run lint && npm run build:guards && npm run bundle:index && npm run bundle:mcp && npm run build:install",
    "release": "npm ci && npm run build && npm run test",
    "start": "node dist/index.js",
    "inspect": "node bin/inspect.mjs",
    "bundle:index": "tsc --noEmit && esbuild src/index.ts --outfile=dist/index.js --platform=node --bundle --target=node22 --format=cjs --external:fs --minify",
    "bundle:mcp": "tsc --noEmit && esbuild src/mcp/server.ts --outfile=dist/mcp.js --platform=node --bundle --target=node22 --format=cjs --external:fs --minify"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.41.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-auto-guard": "^5.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "dotenv": "^17.2.1",
    "streeteasy-api": "^0.1.11",
    "zod": "^3.25.76"
  }
}
